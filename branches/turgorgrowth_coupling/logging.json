{
    "version": 1,
    "formatters": {
        "verbose": {
            "format": "%(levelname)s - %(name)s - %(message)s"
        },
        "simple": {
            "format": "%(message)s"
        }
    },
    "handlers": {
        "console": {
            "level": "WARNING",
            "class": "logging.StreamHandler",
            "stream": "ext://sys.stdout",
            "formatter": "verbose"
        },
        "file_info": {
            "level": "INFO",
            "class": "logging.handlers.RotatingFileHandler",
            "filename": "info.log",
            "maxBytes": 10485760,
            "backupCount": 20,
            "mode": "w",
            "formatter": "verbose"
        },
        "file_debug": {
            "level": "DEBUG",
            "class": "logging.handlers.RotatingFileHandler",
            "filename": "debug.log",
            "maxBytes": 10485760,
            "backupCount": 20,
            "mode": "w",
            "formatter": "verbose"
        },
        "Limbe_emerge_compartments_file": {
            "level": "DEBUG",
            "class": "logging.handlers.RotatingFileHandler",
            "filename": "Limbe_emerge_compartments.csv",
            "maxBytes": 10485760,
            "backupCount": 20,
            "mode": "w",
            "formatter": "simple"
        },
        "phloeme_compartments_file": {
            "level": "DEBUG",
            "class": "logging.handlers.RotatingFileHandler",
            "filename": "phloeme_compartments.csv",
            "maxBytes": 10485760,
            "backupCount": 20,
            "mode": "w",
            "formatter": "simple"
        },
        "HiddenGrowingZone_compartments_file": {
            "level": "DEBUG",
            "class": "logging.handlers.RotatingFileHandler",
            "filename": "HiddenGrowingZone_compartments.csv",
            "maxBytes": 10485760,
            "backupCount": 20,
            "mode": "w",
            "formatter": "simple"
        },
        "croissance_compartments_file": {
            "level": "DEBUG",
            "class": "logging.handlers.RotatingFileHandler",
            "filename": "croissance_compartments.csv",
            "maxBytes": 10485760,
            "backupCount": 20,
            "mode": "w",
            "formatter": "simple"
        },
        "Limbe_emerge_derivatives_file": {
            "level": "DEBUG",
            "class": "logging.handlers.RotatingFileHandler",
            "filename": "Limbe_emerge_derivatives.csv",
            "maxBytes": 10485760,
            "backupCount": 20,
            "mode": "w",
            "formatter": "simple"
        },
        "phloeme_derivatives_file": {
            "level": "DEBUG",
            "class": "logging.handlers.RotatingFileHandler",
            "filename": "phloeme_derivatives.csv",
            "maxBytes": 10485760,
            "backupCount": 20,
            "mode": "w",
            "formatter": "simple"
        },
        "HiddenGrowingZone_derivatives_file": {
            "level": "DEBUG",
            "class": "logging.handlers.RotatingFileHandler",
            "filename": "HiddenGrowingZone_derivatives.csv",
            "maxBytes": 10485760,
            "backupCount": 20,
            "mode": "w",
            "formatter": "simple"
        },
        "croissance_derivatives_file": {
            "level": "DEBUG",
            "class": "logging.handlers.RotatingFileHandler",
            "filename": "croissance_derivatives.csv",
            "maxBytes": 10485760,
            "backupCount": 20,
            "mode": "w",
            "formatter": "simple"
        }
    },
    "loggers": {
        "elongwheat": {
            "handlers": [
                "console",
                "file_info",
                "file_debug"
            ]
        },
        "elongwheat.model": {
            "handlers": [
                "file_debug"
            ],
            "propagate": false
        },
        "elongwheat.compartments": {
            "propagate": false
        },
        "elongwheat.compartments.Limbe_emerge": {
            "handlers": [
                "Limbe_emerge_compartments_file"
            ]
        },
        "elongwheat.compartments.phloeme": {
            "handlers": [
                "phloeme_compartments_file"
            ]
        },
        "elongwheat.compartments.HiddenGrowingZone": {
            "handlers": [
                "HiddenGrowingZone_compartments_file"
            ]
        },
        "elongwheat.compartments.croissance": {
            "handlers": [
                "croissance_compartments_file"
            ]
        },
        "elongwheat.derivatives": {
            "propagate": false
        },
        "elongwheat.derivatives.Limbe_emerge": {
            "handlers": [
                "Limbe_emerge_derivatives_file"
            ]
        },
        "elongwheat.derivatives.phloeme": {
            "handlers": [
                "phloeme_derivatives_file"
            ]
        },
        "elongwheat.derivatives.HiddenGrowingZone": {
            "handlers": [
                "HiddenGrowingZone_derivatives_file"
            ]
        },
        "elongwheat.derivatives.croissance": {
            "handlers": [
                "croissance_derivatives_file"
            ]
        }
    }
}